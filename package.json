    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.1"
  }
}
