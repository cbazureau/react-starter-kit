{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src/**/*.jsx",
    "lint-fix": "./node_modules/.bin/eslint ./src/**/*.jsx --fix",
    "dev": "export PROD_ENV=0;./node_modules/.bin/webpack-dev-server --hot --inline --watch",
    "clean": "rm -r dist/",
    "test": "jest --config ./tools/jest.json",
    "build": "export PROD_ENV=1;./node_modules/.bin/webpack && cp index.html dist/index.html",
    "sass": "sass --watch src/main.scss:dist/main.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
